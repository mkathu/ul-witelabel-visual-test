{
  "config": {
    "settings": {
      "onlyCategories": ["performance","accessibility","bestPractices","performance","progressiveWebApp","seo"],
      "onlyAudits": [
      "first-meaningful-paint",
      "speed-index",
      "interactive",
      "cumulative-layout-shift",
      "largestContentfulPaint"
    ]
    }
  },
  "options": {
    "chromeFlags": [
      "--disable-dev-shm-usage",
      "--headless",
      "--no-sandbox",
      "--ignore-certificate-errors"
    ]
  }
}
